{
  "name": "Agno Agent Dev",
  "image": "artifacts.drwholdings.com/docker/devcontainers/python:3.11",
  "postCreateCommand": "pip install --quiet agno openai python-dotenv",
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}
